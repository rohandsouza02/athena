# Athena Bot Environment Configuration
# Copy this file to .env and fill in your actual values

# Vexa AI API Configuration
VEXA_API_KEY=your_vexa_api_key_here

# Google Calendar API Configuration
GOOGLE_CREDENTIALS_PATH=credentials.json
GOOGLE_CALENDAR_ID=primary

# Webhook Configuration
WEBHOOK_URL=https://your-webhook-endpoint.com/transcript
WEBHOOK_SECRET=your_webhook_secret_here

# OpenAI API (if using enhanced LLM features)
OPENAI_API_KEY=your_openai_api_key_here

# Slack Webhook (if using Slack integration)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK